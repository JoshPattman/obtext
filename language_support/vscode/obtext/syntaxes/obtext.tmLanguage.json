{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "obtext",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.obtext",
					"match": "(?<!\\\\)@[a-zA-Z_][a-zA-Z0-9_]*"
				}
			]
		}
	},
	"scopeName": "source.obt"
}